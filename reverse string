public class seven {
	
	public static void main(String[] args) {
     String s="kaviya.is.good.girl";
		int i = s.length() - 1, j = s.length() - 1;
		do
		{
			while(j>=0 && s.charAt(j)!='.' )
			{
				j--;
				
			}
				
			System.out.print(s.substring(j+1,i+1)+" ");
				
			j--;
			i=j;
		}while(j>=0);
	}
}
