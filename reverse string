public class seven {
	
	public static void main(String[] args) {
     String s="kaviya.is.good.girl";
		String answer="";
		String flag="";
		for(int index=str.length()-1;index>=0;index--)
		{
			if(str.charAt(index)!='.')
			{
				flag=str.charAt(index)+flag;
			}
			else
			{
				answer=answer+flag+str.charAt(index);
				flag="";
			}
		}
		answer+=flag;
		System.out.println(answer);
	}
}
